
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
   
    <div>
        @{
            ViewBag.Title = "Listado de Productos";
        }
        @model IEnumerable<Proyecto5_Diplomado_Web_MVC_UASD.Models.Producto>

        <table>
            <tr>
                <th>
                    Id
                </th>
                <th></th>
                <th>
                    Descripcion
                </th>
                <th></th>
                <th>
                    Precio
                </th>
                <th></th>
                

            </tr>
            @foreach (var item in Model)
            {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Id)
                </td>
                <th></th>
                <td>
                    @Html.DisplayFor(modelItem => item.Descripcion)
                </td>
                <th></th>
                <td>
                    @Html.DisplayFor(modelItem => item.Precio)
                </td>
                <th></th>
               
            </tr>
            }
        </table>
    </div>
</body>
</html>
